  "devDependencies": {
    "conventional-changelog-cli": "^2.2.2",
    "sass": "^1.58.1",
    "vite": "^4.1.1",
    "vite-plugin-dts": "^1.7.2",
    "vite-plugin-libcss": "^1.0.5",
    "vue-tsc": "^1.0.24"
  },
  "bugs": {
    "url": "https://github.com/tomiaa12/latex-template/issues"
  },
  "homepage": "https://github.com/tomiaa12/latex-template#readme",
  "repository": {
